#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main(){
ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
     int t;
     cin>>t;
     while(t--){
        int a,b,c;
        cin>>a>>b>>c;
        int one=0 , two=0 , three=0;
        if((a-b)%2 == 0 ){
            three=1;
        }
        if((b-c)%2 == 0){ 
            one=1;
        }
        if((c-a)%2 == 0){
            two=1;
        }

        cout<<(one&1)<<" "<<(two&1)<<" "<<(three&1)<<endl;

    }
     return 0;
}
